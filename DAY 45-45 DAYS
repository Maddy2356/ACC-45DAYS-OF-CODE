import java.util.Scanner;

class PresentsForCheffina {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int T = scanner.nextInt();
        while (T-- > 0) {
            long N = scanner.nextLong();
            System.out.println(N - (N / 5));
        }
        scanner.close();
    }
}
