import java.util.Scanner;

class MaxTastiness {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt(); 

        for (int i = 0; i < T; i++) {
            int a = sc.nextInt(); 
            int b = sc.nextInt(); 
            int c = sc.nextInt();
            int d = sc.nextInt(); 

            int maxTastiness = Math.max(a, b) + Math.max(c, d);

            System.out.println(maxTastiness);
        }
    }
}
